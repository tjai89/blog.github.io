<h1 id="page_title">This is my projects</h1>


<% if user_signed_in? %>

	<div id="admin_links" class="create">
		<%= link_to 'Create new project', new_project_path, :class => "create" %>
	</div>
<% end %>

<div id="posts_wrapper" class="skinny_wrapper">
	<% @projects.each do |project| %>
		<div class="post">
			<p class="date"><%= project.created_at.strftime('%A, %Y %B %d') %></p>
			<h2><a href="#"><%= link_to project.title, project %></a></h2>
			<hr>
		</div>
	<% end %>
</div>